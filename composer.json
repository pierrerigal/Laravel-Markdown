{
    "name": "graham-campbell/markdown",
    "description": "Markdown Is A CommonMark Wrapper For Laravel 5",
    "keywords": ["laravel", "framework", "markdown", "commonmark", "common mark", "Laravel Markdown", "Laravel-Markdown", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/framework"
        },
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/html"
        },
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/testbench"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/support": "~5.0",
        "illuminate/view": "~5.0",
        "league/commonmark": "0.6.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Markdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Markdown\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
